    "build": "webpack --mode production",
    "deploy": "gh-pages -d dist/examples",
    "publish-demo": "npm run build && npm run deploy"
  },
  "license": "MIT",
  "devDependencies": {
    "lerna": "^2.11.0"
  }
}
